{
  "name": "flutter-widget-catcher",
  "version": "1.0.0",
  "description": "Chrome extension to easily copy Flutter widgets with proper bracket matching",
  "private": true,
  "author": "Flutter Widget Catcher Team",
  "license": "MIT",
  "homepage": "https://github.com/your-username/flutter-widget-catcher",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/flutter-widget-catcher.git"
  },
  "keywords": [
    "flutter",
    "chrome-extension",
    "widget",
    "development-tools",
    "dart",
    "code-copy"
  ],
  "scripts": {
    "watch": "webpack --mode=development --watch --config config/webpack.config.js",
    "build": "webpack --mode=production --config config/webpack.config.js",
    "build:dev": "webpack --mode=development --config config/webpack.config.js",
    "clean": "rm -rf dist build",
    "lint": "eslint src/ --ext .js",
    "lint:fix": "eslint src/ --ext .js --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "package": "npm run build && npm run create-package",
    "create-package": "node scripts/package.js",
    "release": "npm run build && npm run create-package && node scripts/upload-to-store.js",
    "version:patch": "npm version patch && git push && git push --tags",
    "version:minor": "npm version minor && git push && git push --tags",
    "version:major": "npm version major && git push && git push --tags"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "^4.3.0",
    "eslint": "^8.0.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.25.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "file-loader": "^6.2.0",
    "jest": "^29.0.0",
    "mini-css-extract-plugin": "^0.10.1",
    "size-plugin": "^2.0.2",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^5.8.0",
    "chrome-webstore-upload": "^3.0.0",
    "archiver": "^5.3.0"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  }
}
